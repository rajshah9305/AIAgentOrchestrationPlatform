{
  "version": 2,
  "name": "ai-agent-orchestrator-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_WS_URL": "@ws_url"
  },
  "functions": {
    "frontend/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
} 